{
  "version": 2,
  "routes": [
    { "src": "^/assets/(.*)$", "dest": "/assets/$1" },
    { "src": "^/api/(chat|chat-live|chat-edge)(?:/)?$", "dest": "/api/chat2" },
    { "src": "^/api/(chat|chat-live|chat-edge)/(.*)$",  "dest": "/api/chat2/$2" },
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "^(?!/api|/assets).*$", "dest": "/index.html" }
  ]
}